{
  "name": "csv-aligner",
  "displayName": "csv-aligner",
  "description": "aligns csvs using inlay hints",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:csv"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gsmithapps/csv-aligner"
  },
  "main": "./out/extension.js",
  "contributes": {
  "languages": [
    {
      "id": "csv",
      "aliases": ["CSV", "csv"],
      "extensions": [".csv", ".tsv"]
    }
  ],
  "inlayHints": [
    {
      "language": "csv"
    }
  ]
  },
  "icon": "images/CSV-Aligner-icon.png",
  "publisher": "GrantSmith",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "doc": "typedoc --out docs --entryPointStrategy expand src"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/papaparse": "^5.3.14",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typedoc": "^0.28.12",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "papaparse": "^5.4.1"
  }
}
